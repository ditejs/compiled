    "**/dist/**"
  ],
  "include": [
    ".",
    "jest"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
